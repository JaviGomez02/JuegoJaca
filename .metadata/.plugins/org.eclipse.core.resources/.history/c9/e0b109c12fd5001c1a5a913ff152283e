package elementos;

public class Coordenada {

	private int x;
	private int y;
	public Coordenada(int x, int y) {
		super();
		this.x = x;
		this.y = y;
	}
	
	
	public Coordenada() {
		super();
		this.x = 0;
		this.y = 0;
	}


	private void setX(int x) {
		this.x = x;
	}


	private void setY(int y) {
		this.y = y;
	}
	
}
